{
  "name": "rest-express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "psm-gen-dev": "prisma generate --schema=prisma/dev/schema.prisma",
    "psm-gen-prod": "prisma generate --schema=prisma/prod/schema.prisma",
    "psm-pull-db-dev": "prisma db pull --schema=prisma/dev/schema.prisma",
    "psm-pull-db-prod": "prisma db pull --schema=prisma/prod/schema.prisma",
    "PM2-dev": "pm2 start ./config/pm2/pm2-dev-process.config.js",
    "tt": "tsc index.ts -w & pm2 start dist/app.js --watch",
    "start:dev3": "nodemon src/index.ts localhost 3333",
    "start:dev": "pm2 start ./config/pm2/pm2-dev-process.config.js --watch",
    "start:prod": "pm2 start ./config/pm2/pm2-prod-process.config.js",
    "go:dev": "yarn psm-pull-db-dev&&yarn psm-gen-dev&&yarn nodemon src/server.ts",
    "go:nodemon": "nodemon src/server.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "express": "4.17.3",
    "helmet": "^5.0.2",
    "moment-timezone": "^0.5.34",
    "pm2": "^5.2.0"
  },
  "devDependencies": {
    "@prisma/client": "^3.11.1",
    "@types/express": "4.17.13",
    "@types/node": "16.11.26",
    "moment": "^2.29.1",
    "nodemon": "^2.0.15",
    "prisma": "3.11.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
